+msgstr "SYNOPSIS"
+
+# type: Plain text
+#: accept.man:25
+msgid ""
+"B<accept> [ -E ] [ -U I<username> ] [ -h I<hostname[:port]> ] destination(s)"
+msgstr ""
+"B<accept> [ -E ] [ -U I<utilisateur> ] [ -h I<serveur[:port]> ] destination"
