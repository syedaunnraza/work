+
+# type: Plain text
+#: accept.man:34
+msgid ""
+"B<reject> [ -E ] [ -U I<username> ] [ -h I<hostname[:port]> ] [ -r "
+"I<reason> ] destination(s)"
+msgstr ""
+"B<reject> [ -E ] [ -U I<utilisateur> ] [ -h I<serveur[:port]> ] [ -r "
+"I<raison> ] destination(s)"
